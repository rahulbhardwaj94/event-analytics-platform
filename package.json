    "rate-limit-redis": "^4.2.0",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.11",
    "bull": "^4.12.0",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
